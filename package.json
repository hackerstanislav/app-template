{
  "name": "app-template",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "server/build": "cd server && tsc --outDir ./dist",
    "server/development": "cd server && tsc --outDir ./dist --watch",
    "server/development/database": "cd server && node database.js",
    "server/lint": "cd server && tsc --noEmit && eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "server/start": "cd server && node ./dist/main/index.js start 8080 --staticFolder ../client/web/dist --env ../.env",

    "client/web/build": "cd client/web && tsc && vite build",
    "client/web/development": "cd client/web && vite --force",
    "client/web/lint": "cd client/web && tsc --noEmit && eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "client/web/extract-locales": "i18next-scanner --config client/i18next-scanner.config.cjs && prettier --write client/web/sources/languages/*.json",

    "prettier": "prettier --write {client,server}/**/*",
    "lint": "prettier --check {client,server}/**/* && npm run server/lint && npm run client/web/lint",

    "version-patch": "npm version patch -m \"Release %s\"",
    "version-minor": "npm version minor -m \"Release %s\"",
    "version-major": "npm version major -m \"Release %s\""
  },
  "private": true,
  "dependencies": {
    "dotenv": "^16.4.5",
    "@kinde-oss/kinde-typescript-sdk": "^2.8.0",
    "chalk": "^4.1.2",
    "connect-mongodb-session": "^3.1.1",
    "cookie-parser": "^1.4.6",
    "node-cache": "^5.1.2",
    "dayjs": "^1.11.10",
    "envfull": "^1.0.5",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.17.3",
    "express-socket.io-session": "^1.3.5",
    "mongodb": "^6.0.0",
    "openapi-validator-middleware": "^3.2.6",
    "socket.io": "^4.7.2",
    "lodash": "^4.17.21",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.12",
    "@mui/icons-material": "^5.15.12",
    "@mui/lab": "^5.0.0-alpha.168",
    "@mui/material": "^5.15.14",
    "@mui/system": "^5.15.14",
    "@mui/x-date-pickers": "^7.0.0",
    "i18next": "^23.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "js-cookie": "^3.0.5"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "@types/gtag.js": "^0.0.19",
    "@types/connect-mongodb-session": "^2.4.5",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/express-socket.io-session": "^1.3.7",
    "@types/node": "^18.17.12",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@types/jasmine": "^5.1.4",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "eslint": "^8.57.1",
    "openapi-typescript": "^5.4.2",
    "prettier": "3.2.5",
    "typescript": "^5.2.2",
    "jasmine": "^5.2.0",
    "jasmine-spec-reporter": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.19",
    "i18next-scanner": "^4.4.0",
    "i18next-scanner-typescript": "^1.2.1",
    "vite": "^5.1.6",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-mkcert": "^1.17.6",
    "vitest": "^2.0.5"
  }
}
